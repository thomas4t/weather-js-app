  "scripts": {
    "predev": "yarn locale-compile",
    "dev": "razzle start",
    "prebuild": "yarn locale-compile",
    "build": "razzle build --noninteractive",
    "test": "jest --passWithNoTests --verbose",
    "start:prod": "NODE_ENV=production node build/server.js",
    "prestorybook": "yarn locale-compile",
    "storybook": "start-storybook -p 6006",
    "prestorybook:build": "yarn locale-compile",
    "storybook:build": "build-storybook",
    "lint": "echo 'ls-lint ...' && ls-lint && echo 'detecting copy & paste ...' && jscpd src && echo 'eslint ...' && eslint src --ext .ts,.tsx --fix && echo 'tsc ...' && tsc --noEmit",
    "icons:generate": "npx @svgr/cli@^6.2.0 --config-file .svgr.config.js src/components/elements/Icon/assets --out-dir src/components/elements/Icon/icons",
    "graphqltypings:generate": "graphql-codegen && eslint src/typings/graphql* --fix",
    "locale-extract": "lingui extract",
    "locale-compile": "lingui compile",
    "generate": "plop --plopfile plopfile.js",
    "test:generate": "yarn generate component myName element && yarn generate page MyPage /fooo 'Heading' && yarn generate store fetchSaga MyName && yarn generate store moduleSaga MyName && yarn generate store duck MyName && yarn lint"
  },
