{
  "name": "weather-js-appbackend",
  "version": "null",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=v16.13.0 <17"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "rm -rf dist",
    "start": "env-cmd --fallback --silent -f ../.env nest start",
    "predev": "rm -rf dist",
    "dev": "env-cmd --fallback --silent -f ../.env nest start --watch",
    "predev:debug": "rm -rf dist",
    "dev:debug": "env-cmd --fallback --silent -f ../.env nest start --debug --watch",
    "prestart:prod": "yarn migrations:run || true",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "echo 'ls-lint ...' && ls-lint && echo 'detecting copy & paste ...' && jscpd src && echo 'eslint ...' && eslint \"{src,apps,libs,test}/**/*.ts\" --fix && echo 'tsc ...' && tsc --noEmit",
    "test": "jest",
    "test:ci": "yarn test --coverage --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd --fallback --silent -f ../.env --use-shell 'yarn schema:drop && yarn fixtures:load && jest --config ./src/jest-e2e.json'",
    "test:e2e:ci": "yarn schema:drop && yarn fixtures:load && jest --config ./src/jest-e2e.json --coverage --reporters=default --reporters=jest-junit",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./ormconfig.ts schema:sync",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./ormconfig.ts schema:drop",
    "schema:diagram": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm-uml ormconfig.ts --format=svg",
    "fixtures:load": "ts-node -r tsconfig-paths/register ./node_modules/.bin/fixtures ./src/App/**/*.fixtures.yml -c ./ormconfig.fixtures.ts --sync --debug",
    "migration:create": "typeorm migration:create src/migrations/migration$(date \"+%s\")",
    "migration:generate": "env-cmd --fallback --silent -f ../.env ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -d ./ormconfig.ts src/migrations/migrationBatch$(date \"+%s\").ts",
    "migrations:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./ormconfig.ts migration:run",
    "data-recreate": "env-cmd --fallback --silent -f ../.env --use-shell 'yarn schema:drop && yarn fixtures:load'",
    "data-recreate:ci": "yarn schema:drop && yarn fixtures:load"
  },
  "dependencies": {
    "@nestjs/apollo": "10.0.8",
    "@nestjs/common": "8.4.2",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.2",
    "@nestjs/cqrs": "8.0.3",
    "@nestjs/event-emitter": "1.1.0",
    "@nestjs/graphql": "10.0.8",
    "@nestjs/platform-express": "8.4.2",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/typeorm": "8.0.3",
    "@sentry/node": "6.19.1",
    "apollo-server-express": "3.6.5",
    "class-validator": "0.13.2",
    "crypto-js": "4.1.1",
    "graphql": "16.3.0",
    "graphql-relay": "0.10.0",
    "jsonwebtoken": "8.5.1",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "ts-morph": "14.0.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typeorm": "0.3.2",
    "typeorm-fixtures-cli": "1.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@inventi/eslint-config-node": "1.0.1",
    "@inventi/eslint-plugin-doc": "1.0.2",
    "@ls-lint/ls-lint": "1.11.0",
    "@nestjs/cli": "8.2.4",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.2",
    "@types/cron": "1.7.3",
    "@types/crypto-js": "4.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.22",
    "@types/supertest": "2.0.12",
    "env-cmd": "10.1.0",
    "eslint": "8.11.0",
    "jest": "27.5.1",
    "jest-junit": "13.0.0",
    "jscpd": "3.4.5",
    "prettier": "2.6.0",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.8",
    "typeorm-uml": "1.6.5",
    "typescript": "4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "@App/(.*)$": "<rootDir>/App/$1",
      "@Common/(.*)$": "<rootDir>/Common/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
